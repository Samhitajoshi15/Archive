#include<iostream>
#include<cmath>
using namespace std;
int diagonalDifference(int arr[][n],int n)
{
    int i,j,d1=0,d2=0;

    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            if(i==j)
            d1++;
            if(i+j==n-1)
            d2++;
        }
    }
    int abd = abs(d1-d2);
    return abd;
}
int main()
{
    int n,i,j;
    cin>>n;
    int arr[n][n];
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        cin>>arr[i][j];
    }
    cout<<diagonalDifference(arr,n);
    return 0;
}
