https://www.hackerrank.com/challenges/mini-max-sum/problem

static void miniMaxSum(int[] arr) {
    Arrays.sort(arr);
    int minsum=0,maxsum=0,sum=0;
    for(int j=0;j<5;j++)
    {
      sum+=arr[j];
    }
    minsum= sum-arr[4];
    maxsum= sum-arr[0];

    System.out.print(minsum+" "+maxsum);

    }
    
    
    
    //I made this in C++
    #include <iostream>
#include <bits/stdc++.h> 
using namespace std;
int i,j;
void minmaxsum(long long int arr[])
{
    long long int sum=0,minsum=0,maxsum=0;
    for(int j=0;j<5;j++)
    sum+=arr[j];
    minsum= sum-arr[4];
    maxsum= sum-arr[0];
    cout<<minsum<<" "<<maxsum;
}
int main()
{
 long long int arr[5];
 for(i=0;i<5;i++)
 cin>>arr[i];
sort(arr, arr+5);
minmaxsum(arr);
 return 0;
}
