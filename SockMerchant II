#include <iostream>
using namespace std;
int i;
int sockMerchant(int ar[],int n)
{
    int sell=0,count[100]={0};
    int j,val;
    for(i=0;i<n;i++)
    {
      val =ar[i];
      if(val!=0)
      {
      for(j=0;j<n;j++)
      { 
            if(ar[i]==ar[j])
            {
           count[ar[i]]++;
            }
      }
      for(j=0;j<n;j++)
      {
       if(ar[j]==val)
       ar[j]=0;
      }
      }
    }
    for(i=0;i<n;i++)
    {
        sell+=count[ar[i]]/2;
    }
    return sell;
}
int main()
{
    int n;
    cin>>n;
    int ar[n];
    for(i=0;i<n;i++)
    cin>>ar[i];
    cout<<sockMerchant(ar,n);
    return 0;
}
